{{- $latest       := index site.Data.releases 1 -}}
{{- $allDocs      := where site.RegularPages "Section" "docs" }}
{{- $docRedirects := site.Data.redirects }}

# Redirect to latest doc version by default
/docs     /docs/latest


{{- range $docRedirects }}
/docs/{{ . }}     /docs/latest/{{ . }}

# Legacy git book redirects
/docs/{{ . }}.html /docs/latest/{{ . }}
{{- end }}
